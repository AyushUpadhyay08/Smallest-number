#to find the smallest number among three numbers.
import java.util.*;
class J
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int a, b, c;
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        if(a<b && a<c)
            System.out.print("a is smaller than b and c");
        else if(b<a && b<c)
            System.out.print("b is smaller than a and c");
        else if(c<a && c<b)
            System.out.print("c is smaller than a and b");
        else
            System.out.print("all three are equal");        
    }
}
